{
  "name": "use-trpc",
  "version": "2.0.0-beta.1",
  "description": "Vue composable for tRPC v10",
  "repository": "github:aphex/use-trpc",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest run"
  },
  "keywords": [
    "trpc",
    "vue",
    "composable",
    "reactive"
  ],
  "author": "Ross Gerbasi",
  "license": "ISC",
  "dependencies": {
    "@trpc/client": "^10.9.1",
    "@trpc/server": "^10.9.1",
    "vue-demi": "*"
  },
  "devDependencies": {
    "@types/ws": "^8.5.4",
    "happy-dom": "^8.2.6",
    "unbuild": "^1.1.1",
    "vitest": "^0.28.4",
    "ws": "^8.12.0",
    "zod": "^3.20.2"
  },
  "workspaces": [
    ".",
    "examples/**/*"
  ],
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vue"
      ]
    }
  }
}
